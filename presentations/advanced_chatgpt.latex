\section{Analytical Prompting Techniques}

\subsection{Debate Simulation}
\begin{frame}{Debate Simulation}
    \begin{itemize}
        \item \textbf{Definition:} Frame the topic as a \highlighttext{debate}, prompting the model to present \highlighttext{arguments} from \highlighttext{multiple sides}.
        \item \textbf{Explanation:} Like a \highlighttext{courtroom debate}, this technique encourages \highlighttext{balanced views}, providing arguments both \highlighttext{for} and \highlighttext{against}.
        \item \textbf{Example:} “Discuss the \alerttext{pros and cons} of \highlighttext{nuclear energy}.”
        \item \textbf{Benefit:} Encourages \highlighttext{balanced analysis} by examining \highlighttext{different perspectives} on the same issue.
    \end{itemize}
\end{frame}

\subsection{Cross-Disciplinary Queries}
\begin{frame}{Cross-Disciplinary Queries}
    \begin{itemize}
        \item \textbf{Definition:} Combine elements from different \highlighttext{disciplines} to explore a topic from \highlighttext{multiple perspectives}.
        \item \textbf{Explanation:} This is like bringing together \highlighttext{specialists} from various fields to examine an idea, enriching the response through \highlighttext{diverse approaches}.
        \item \textbf{Example:} “Relate \highlighttext{quantum physics} to \highlighttext{philosophy}.”
        \item \textbf{Benefit:} Promotes \highlighttext{comprehensive responses} by linking concepts across \highlighttext{domains}.
    \end{itemize}
\end{frame}

\subsection{Recursive Exploration}
\begin{frame}{Recursive Exploration}
    \begin{itemize}
        \item \textbf{Definition:} Continue exploring each response by asking \highlighttext{follow-up questions} about the \highlighttext{previous answer}.
        \item \textbf{Explanation:} Like \highlighttext{drilling down} through layers of information, recursive exploration digs deeper into the topic with each question, \highlighttext{refining understanding}.
        \item \textbf{Example:} “Tell me about X. Why did X happen? What’s the significance of that reason?”
        \item \textbf{Benefit:} Enables \highlighttext{thorough investigation} of \highlighttext{complex ideas} by pursuing each layer of inquiry.
    \end{itemize}
\end{frame}

\subsection{Futurecasting}
\begin{frame}{Futurecasting}
    \begin{itemize}
        \item \textbf{Definition:} Prompt the model to make \highlighttext{predictions} or \highlighttext{projections} based on current \highlighttext{trends}.
        \item \textbf{Explanation:} Like \highlighttext{forecasting the weather}, futurecasting projects likely outcomes by examining current \highlighttext{patterns} and \highlighttext{extrapolating} them into the future.
        \item \textbf{Example:} “Given the current trend in \highlighttext{AI}, what will \highlighttext{technology} look like in \alerttext{2050}?”
        \item \textbf{Benefit:} Helps explore \highlighttext{possible future developments}, \highlighttext{scenarios}, and their \highlighttext{implications}.
    \end{itemize}
\end{frame}

\subsection{Backstory Creation}
\begin{frame}{Backstory Creation}
    \begin{itemize}
        \item \textbf{Definition:} Prompting the model to generate detailed \highlighttext{histories} for \highlighttext{fictional characters}.
        \item \textbf{Explanation:} Similar to crafting \highlighttext{character backgrounds} in novels or films, backstory creation breathes life into \highlighttext{fictional personas}, establishing \highlighttext{depth} and \highlighttext{personality}.
        \item \textbf{Example:} “Create a \alerttext{backstory} for a pirate named \highlighttext{Captain Redbeard}.”
        \item \textbf{Benefit:} Useful for \highlighttext{storytelling}, \highlighttext{character development}, and enhancing \highlighttext{creative writing}.
    \end{itemize}
\end{frame}

\subsection{Concept Fusion}
\begin{frame}{Concept Fusion}
    \begin{itemize}
        \item \textbf{Definition:} Merging two \highlighttext{unrelated concepts} and asking the model to describe the resulting \highlighttext{fusion}.
        \item \textbf{Explanation:} Like \highlighttext{blending} two colors to create a new shade, concept fusion combines distinct ideas to create \highlighttext{novel perspectives} or \highlighttext{imaginative possibilities}.
        \item \textbf{Example:} “Combine the concepts of ‘\highlighttext{time travel}’ and ‘\highlighttext{photography}’. What do you get?”
        \item \textbf{Benefit:} Encourages \highlighttext{innovation} and \highlighttext{creative thinking} by exploring the \highlighttext{intersection} of seemingly \highlighttext{unrelated ideas}.
    \end{itemize}
\end{frame}

\subsection{Feedback Iteration}
\begin{frame}{Feedback Iteration}
    \begin{itemize}
        \item \textbf{Definition:} Rate the model’s \highlighttext{responses} and use these \highlighttext{scores} to guide subsequent prompts.
        \item \textbf{Explanation:} Similar to \highlighttext{performance reviews}, where feedback and ratings help guide improvement over time, feedback iteration allows for \highlighttext{grading} and \highlighttext{adjusting responses}.
        \item \textbf{Example:} “On a scale of 1-10, your last response was a \alerttext{7}. Can you improve on that?”
        \item \textbf{Benefit:} Supports \highlighttext{quality control} by providing \highlighttext{clear feedback} for \highlighttext{incremental improvement}.
    \end{itemize}
\end{frame}

\subsection{Prompt Mirroring}
\begin{frame}{Prompt Mirroring}
    \begin{itemize}
        \item \textbf{Definition:} Use the model’s own \highlighttext{wording} in \highlighttext{follow-up prompts} to maintain \highlighttext{consistency} and \highlighttext{flow}.
        \item \textbf{Explanation:} Like repeating someone’s words to keep the \highlighttext{conversation aligned}, prompt mirroring leverages existing \highlighttext{phrasing} to encourage \highlighttext{cohesive responses}.
        \item \textbf{Example:} “You mentioned ‘\highlighttext{neural pathways}’. Can you expand on \highlighttext{neural pathways}?”
        \item \textbf{Benefit:} Enhances \highlighttext{coherence} by maintaining \highlighttext{continuity} with the model’s prior \highlighttext{language}.
    \end{itemize}
\end{frame}

\subsection{In-Context Learning}
\begin{frame}{In-Context Learning}
    \begin{itemize}
        \item \textbf{Definition:} \highlighttext{LLMs} can learn from the \highlighttext{context} provided within a single \highlighttext{session}, adjusting responses based on \highlighttext{previous interactions}.
        \item \textbf{Example:} Adapting its \highlighttext{tone} to be more \highlighttext{formal} after being asked to “keep it \highlighttext{professional}” during a conversation.
        \item \textbf{Impact:} Allows for \highlighttext{dynamic}, \highlighttext{session-based learning} without \highlighttext{retraining}, personalizing \highlighttext{interactions} based on prior \highlighttext{cues}.
    \end{itemize}
\end{frame}

\subsection{Systematic Perturbation}
\begin{frame}{Systematic Perturbation}
    \begin{itemize}
        \item \textbf{Definition:} This involves varying elements of a \highlighttext{prompt} to observe how the model reacts differently.
        \item \textbf{Explanation:} Like performing an \highlighttext{experiment} where you change one \highlighttext{variable} at a time to see its impact, systematic perturbation helps discover \highlighttext{subtle differences} in responses.
        \item \textbf{Example:} “What’s the \highlighttext{best way} to cook an \highlighttext{egg}?” vs. “What’s the \highlighttext{best way} to \highlighttext{boil} an egg?”
        \item \textbf{Benefit:} Helps understand how specific \highlighttext{wording changes} affect the model’s \highlighttext{interpretation} and \highlighttext{responses}.
    \end{itemize}
\end{frame}

\subsection{Abstract to Concrete}
\begin{frame}{Abstract to Concrete}
    \begin{itemize}
        \item \textbf{Definition:} Prompts that ask ChatGPT to convert \highlighttext{abstract ideas} into \highlighttext{specific}, \highlighttext{relatable examples}.
        \item \textbf{Explanation:} Like turning a \highlighttext{concept} into a \highlighttext{relatable story}, this approach grounds \highlighttext{complex topics} in \highlighttext{real-world situations}.
        \item \textbf{Example:} “Explain \highlighttext{democracy} through a \alerttext{real-world example}.”
        \item \textbf{Benefit:} Makes \highlighttext{complex} or \highlighttext{abstract ideas} more \highlighttext{understandable} and \highlighttext{engaging}.
    \end{itemize}
\end{frame}

\subsection{Bracketed Constraints}
\begin{frame}{Bracketed Constraints}
    \begin{itemize}
        \item \textbf{Definition:} \highlighttext{Bracketed constraints} are specific \highlighttext{limitations} within a prompt, highlighted using \highlighttext{brackets}.
        \item \textbf{Explanation:} Think of these as setting up \highlighttext{guardrails}. Just like a \highlighttext{bike lane} keeps cyclists safe, bracketed constraints focus ChatGPT on \highlighttext{essential details}.
        \item \textbf{Example:} “Describe [\alerttext{without using scientific jargon}] \highlighttext{photosynthesis}.”
        \item \textbf{Benefit:} Helps guide ChatGPT’s \highlighttext{tone}, \highlighttext{complexity}, or \highlighttext{vocabulary} based on \highlighttext{audience needs}.
    \end{itemize}
\end{frame}

\subsection{Specified Format}
\begin{frame}{Specified Format}
    \begin{itemize}
        \item \textbf{Definition:} Requests for answers in a particular \highlighttext{structure} or \highlighttext{format}, like \highlighttext{lists}, \highlighttext{bullet points}, or \highlighttext{paragraphs}.
        \item \textbf{Explanation:} Imagine it as laying out an exact \highlighttext{blueprint} for a builder; format requests help ChatGPT \highlighttext{organize responses} precisely.
        \item \textbf{Example:} “List the \highlighttext{steps} of \highlighttext{photosynthesis} in \alerttext{bullet points}.”
        \item \textbf{Benefit:} Ensures \highlighttext{organized}, \highlighttext{easy-to-read answers} tailored to the desired \highlighttext{presentation style}.
    \end{itemize}
\end{frame}

\subsection{Follow-up Deep Dives}
\begin{frame}{Follow-up Deep Dives}
    \begin{itemize}
        \item \textbf{Definition:} Request \highlighttext{deeper exploration} of a specific \highlighttext{element} from a \highlighttext{previous response}.
        \item \textbf{Explanation:} Like \highlighttext{zooming in} with a magnifying glass, follow-up deep dives encourage the model to unpack \highlighttext{details} on specific parts of an answer.
        \item \textbf{Example:} “You mentioned \highlighttext{photosynthesis} involves \highlighttext{light}. How exactly does \highlighttext{light} play a role?”
        \item \textbf{Benefit:} Fosters \highlighttext{detailed understanding} by prompting \highlighttext{targeted exploration} within \highlighttext{complex topics}.
    \end{itemize}
\end{frame}

\subsection{Temporal Context}
\begin{frame}{Temporal Context}
    \begin{itemize}
        \item \textbf{Definition:} Frame the prompt within a specific \highlighttext{historical} or \highlighttext{futuristic time period}.
        \item \textbf{Explanation:} Like looking through a \highlighttext{time capsule}, temporal context allows the model to respond as if it were within a specific \highlighttext{era}, shaping the \highlighttext{language} and \highlighttext{understanding} accordingly.
        \item \textbf{Example:} “How was the \highlighttext{internet} perceived in the \alerttext{1990s}?”
        \item \textbf{Benefit:} Provides \highlighttext{time-relevant insights}, enabling \highlighttext{historical} or \highlighttext{forward-looking perspectives}.
    \end{itemize}
\end{frame}

\subsection{Cultural Framing}
\begin{frame}{Cultural Framing}
    \begin{itemize}
        \item \textbf{Definition:} Frame responses within a particular \highlighttext{cultural context} to enhance \highlighttext{relevance}.
        \item \textbf{Explanation:} Similar to observing a \highlighttext{custom} from within its originating culture, cultural framing highlights \highlighttext{nuances} and \highlighttext{practices} unique to specific \highlighttext{societies}.
        \item \textbf{Example:} “How is \highlighttext{New Year} celebrated in \alerttext{China}?”
        \item \textbf{Benefit:} Provides \highlighttext{culturally sensitive responses}, enriching \highlighttext{understanding} of \highlighttext{diverse perspectives}.
    \end{itemize}
\end{frame}

\subsection{Geographical Context}
\begin{frame}{Geographical Context}
    \begin{itemize}
        \item \textbf{Definition:} Introduce a \highlighttext{geographical element} to tailor responses to specific \highlighttext{locations}.
        \item \textbf{Explanation:} Like focusing on \highlighttext{local ingredients} in cuisine, geographical context frames responses with \highlighttext{region-specific details}.
        \item \textbf{Example:} “How is \highlighttext{bread} made traditionally in \alerttext{France}?”
        \item \textbf{Benefit:} Adds \highlighttext{authenticity} by reflecting \highlighttext{local traditions} or \highlighttext{regional practices}.
    \end{itemize}
\end{frame}

\subsection{Emotional Context}
\begin{frame}{Emotional Context}
    \begin{itemize}
        \item \textbf{Definition:} Frame the response within an \highlighttext{emotional perspective} to match a desired \highlighttext{tone}.
        \item \textbf{Explanation:} Similar to \highlighttext{storytelling with emotion}, this technique adds an \highlighttext{emotional lens}, allowing the response to be tailored for \highlighttext{impact}.
        \item \textbf{Example:} “Explain \highlighttext{photosynthesis} in a way that would \alerttext{excite a child}.”
        \item \textbf{Benefit:} Engages the reader by adding \highlighttext{empathy} and \highlighttext{emotional resonance} to explanations.
    \end{itemize}
\end{frame}

\subsection{Contextual Shifts}
\begin{frame}{Contextual Shifts}
    \begin{itemize}
        \item \textbf{Definition:} Change the \highlighttext{context} of a previously answered question to explore \highlighttext{alternative perspectives}.
        \item \textbf{Explanation:} Like viewing the same \highlighttext{scene} in different \highlighttext{seasons}, contextual shifts allow the model to reinterpret a response within a new \highlighttext{frame}.
        \item \textbf{Example:} “Earlier you described a day in \highlighttext{summer}. Now, describe the same setting but in \alerttext{winter}.”
        \item \textbf{Benefit:} Expands \highlighttext{understanding} by exploring \highlighttext{multiple scenarios} or \highlighttext{conditions}.
    \end{itemize}
\end{frame}

\subsection{Sensory Emulation}
\begin{frame}{Sensory Emulation}
    \begin{itemize}
        \item \textbf{Definition:} Prompt the model to describe or interpret scenarios based on specific \highlighttext{senses}.
        \item \textbf{Explanation:} Like an artist painting with \highlighttext{sound}, \highlighttext{sight}, or \highlighttext{smell}, sensory emulation adds \highlighttext{depth} by appealing to the \highlighttext{senses}.
        \item \textbf{Example:} “Describe the \highlighttext{smell} of a \alerttext{bustling marketplace}.”
        \item \textbf{Benefit:} Creates \highlighttext{vivid}, \highlighttext{multi-sensory experiences} for \highlighttext{richer descriptions}.
    \end{itemize}
\end{frame}

\subsection{Inversion of Causality}
\begin{frame}{Inversion of Causality}
    \begin{itemize}
        \item \textbf{Definition:} Provide an \highlighttext{effect} and hypothesize possible \highlighttext{causes}.
        \item \textbf{Explanation:} Similar to \highlighttext{reasoning backwards}, this reveals creative \highlighttext{reverse causation}.
        \item \textbf{Example:} “Given \highlighttext{high migration rates}, suggest possible \alerttext{causes}.”
        \item \textbf{Benefit:} Encourages \highlighttext{creative problem-solving} by exploring \highlighttext{cause-and-effect}.
    \end{itemize}
\end{frame}

\subsection{Pattern Recognition with Anomaly Detection}
\begin{frame}{Pattern Recognition with Anomaly Detection}
    \begin{itemize}
        \item \textbf{Definition:} Identify \highlighttext{patterns} and detect \highlighttext{outliers} or \highlighttext{anomalies}.
        \item \textbf{Explanation:} Like finding the \highlighttext{odd one out} in a sequence, it enhances \highlighttext{analytical skills}.
        \item \textbf{Example:} “In the sequence \highlighttext{1, 2, 3, 5, 8}, identify any \alerttext{anomalies}.”
        \item \textbf{Benefit:} Develops \highlighttext{data sensitivity} and \highlighttext{critical observation}.
    \end{itemize}
\end{frame}

\subsection{Sequential Reaction Testing}
\begin{frame}{Sequential Reaction Testing}
    \begin{itemize}
        \item \textbf{Definition:} Modify a \highlighttext{detail} in the prompt to observe the \highlighttext{response change}.
        \item \textbf{Explanation:} Similar to \highlighttext{testing reactions}, this technique highlights \highlighttext{adaptation abilities}.
        \item \textbf{Example:} “Describe \highlighttext{rainy weather}, then describe it as \alerttext{sunny}.”
        \item \textbf{Benefit:} Shows the model’s \highlighttext{sensitivity} to \highlighttext{prompt changes}.
    \end{itemize}
\end{frame}

\subsection{Temporal Cascade Analysis}
\begin{frame}{Temporal Cascade Analysis}
    \begin{itemize}
        \item \textbf{Definition:} Describe an event’s \highlighttext{impact} across \highlighttext{short}, \highlighttext{medium}, and \highlighttext{long-term periods}.
        \item \textbf{Explanation:} Like analyzing \highlighttext{ripple effects}, it reveals \highlighttext{impact over time}.
        \item \textbf{Example:} “Describe the \highlighttext{effects of deforestation} over \alerttext{decades}.”
        \item \textbf{Benefit:} Enhances \highlighttext{temporal understanding} of \highlighttext{consequences}.
    \end{itemize}
\end{frame}
