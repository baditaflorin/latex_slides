\section{Introduction}

\begin{frame}{\structure{Project Background}}
    \begin{itemize}
        \item Scraped \alerttext{1.5 million} 4-letter .com domains availability.
        \item Identified \alerttext{770,795 unregistered} domains.
        \item Extracted data on \highlighttext{character occurrences} and \highlighttext{patterns}.
    \end{itemize}
\end{frame}

\section{Key Findings}

\begin{frame}{1. \alert{Digits Dominate Unregistered Domains}}
    \begin{itemize}
        \item Digits '\textbf{0}' to '\textbf{9}' are among the \alerttext{top 10 most common characters}.
        \item '\textbf{0}' is the \alerttext{most frequent}, appearing in \alerttext{4.21\%} of unregistered domains.
    \end{itemize}
\end{frame}

\begin{frame}{2. \alert{Abundance of Less Common Letters}}
    \begin{itemize}
        \item Letters like '\textbf{q}' and '\textbf{z}' appear frequently due to \highlighttext{lower desirability}.
        \item '\textbf{q}' in \alerttext{88,355 domains}; '\textbf{z}' in \alerttext{86,459 domains}.
    \end{itemize}
\end{frame}

\begin{frame}{3. \alert{Scarcity of Vowels}}
    \begin{itemize}
        \item The most frequently found vowels are:
        \begin{itemize}
            \item '\textbf{a}' is found in \alerttext{64,546 unregistered domains}.
            \item '\textbf{e}' is found in \alerttext{73,000 unregistered domains}.
        \end{itemize}
        \item In contrast, the less common vowels are:
        \begin{itemize}
            \item '\textbf{i}' appears in \alerttext{79,229 unregistered domains}.
            \item '\textbf{o}' appears in \alerttext{84,853 unregistered domains}.
            \item '\textbf{u}' appears in \alerttext{81,913 unregistered domains}.
        \end{itemize}
    \end{itemize}
\end{frame}

\begin{frame}{4. \alert{Repeated Characters in Unregistered Domains}}
    \begin{itemize}
        \item \textbf{\alert{Triple Repeats:}}
        \begin{itemize}
            \item \alerttext{No unregistered domains} with triple repeated characters.
            \item Highlights their \highlighttext{popularity} and \highlighttext{high registration rate}.
        \end{itemize}
        \item \textbf{\alert{Double Repeats:}}
        \begin{itemize}
            \item Unregistered domains with \alerttext{double repeats} do exist.
            \item '\textbf{V}' leads with \alerttext{1,090 domains}, followed by '\textbf{L}' and '\textbf{R}'.
            \item '\textbf{C}' is the most used, with only \alerttext{386 domains left}.
            \item Second most used character is '\textbf{S}', with \alerttext{523 domains left}.
            \item Third is '\textbf{A}', with \alerttext{536 domains left}.
        \end{itemize}
    \end{itemize}
\end{frame}

\begin{frame}{5. \alert{Increasing Sequences Are Rare}}
    \begin{itemize}
        \item Few unregistered domains still have \highlighttext{increasing character sequences} (e.g., '\textbf{abc}', '\textbf{bcd}').
        \item '\textbf{h}' and '\textbf{o}' start the most sequences with \alerttext{16 domains} each.
    \end{itemize}
\end{frame}

\begin{frame}{6. \alert{'SCAM'} \highlight{The Four Least Common Letters}}
    \begin{itemize}
        \item '\textbf{S}' appears in just \alerttext{2.04\%} of unregistered domains.
        \item '\textbf{C}' appears in \alerttext{2.08\%} of unregistered domains.
        \item '\textbf{A}' appears in \alerttext{2.09\%} of unregistered domains.
        \item '\textbf{M}' appears in \alerttext{2.16\%} of unregistered domains.
        \item \textbf{\highlight{Fun Fact}}: These four least common letters spell "\textbf{SCAM}".
        \item Their \highlighttext{low occurrence} in unregistered domains makes them the \alerttext{most registered letters} in domain names.
    \end{itemize}
\end{frame}

